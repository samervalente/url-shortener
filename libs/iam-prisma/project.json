{
  "name": "iam-prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/iam-prisma/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project iam-prisma --web",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "libs/iam-prisma"
      }
    },
    "reset": {
      "command": "prisma migrate reset",
      "options": {
        "cwd": "libs/iam-prisma"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/iam-prisma"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/iam-prisma"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/iam-prisma/**/*.ts"]
      }
    }
  }
}
